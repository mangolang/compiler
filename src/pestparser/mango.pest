// The starting point
file = { number ~ (NEWLINE+ ~ number)* }


string = { "'" ~ [^'] ~ "'" }

boolean = { "true" | "false" }

// A number, either integer or decimal
number = { ("-" | "+")? ~ (ASCII_DIGIT+ ~ "." ~ ASCII_DIGIT+ | ASCII_DIGIT+) }

